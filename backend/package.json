{
  "private": true,
  "name": "backend",
  "version": "0.0.0",
  "description": "",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "lint": "eslint src",
    "test": "jest",
    "validate": "yarn test && yarn lint",
    "prestart": "yarn build",
    "start": "node lib/index.js",
    "prodstart": "node lib/index.js",
    "dev": "nodemon",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts",
    "makemigration": "ts-node ./node_modules/typeorm/cli migration:generate -n Recording -f ./src/ormconfig.ts",
    "migrate": "ts-node ./node_modules/typeorm/cli migration:run -f ./src/ormconfig.ts",
    "migrations": "ts-node ./node_modules/typeorm/cli migration:show -f ./src/ormconfig.ts",
    "createRecording": "ts-node src/bin/takeRecording.ts"
  },
  "author": {
    "name": "David Brownman",
    "email": "beamneocube@gmail.com",
    "url": "https://davidbrownman.com"
  },
  "license": "ISC",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "express": "4.17.1",
    "got": "^11.8.5",
    "helmet": "4.1.1",
    "pg": "^8.4.1",
    "typeorm": "0.2.28"
  },
  "devDependencies": {
    "@types/express": "4.17.8",
    "@types/helmet": "0.0.48",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.10",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-config-xavdid": "1.0.0",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11",
    "eslint-plugin-promise": "^4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4",
    "jest": "26.5.3",
    "nodemon": "^2.0.5",
    "prettier": "2.1.2",
    "ts-jest": "26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "4.0.3"
  }
}
